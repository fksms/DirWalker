{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "macOS": {
      "signingIdentity": "-"
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "DirWalker",
  "mainBinaryName": "DirWalker",
  "version": "0.2.2",
  "identifier": "com.dirwalker.build",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "DirWalker",
        "maximizable": false,
        "width": 864,
        "height": 648,
        "minWidth": 864,
        "minHeight": 648,
        "maxWidth": 1920,
        "maxHeight": 648,
        "useHttpsScheme": true
      }
    ]
  }
}